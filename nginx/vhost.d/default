 error_page   403  /403.html;

 location = /403.html {
         root   /home/dockeruser/web;
 }


location /static/ {
    alias /home/dockeruser/web/staticfiles/;
    add_header Access-Control-Allow-Origin *;
}

location /media/ {
    alias /home/dockeruser/web/mediafiles/;
    add_header Access-Control-Allow-Origin *;
}